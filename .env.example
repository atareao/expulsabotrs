# Configuración de ExpulsaBot

# Token del bot de Telegram (REQUERIDO)
TOKEN=tu_bot_token_aquí

# Duración del challenge en minutos (opcional, por defecto: 2)
CHALLENGE_DURATION_MINUTES=2

# Tratamiento de bots (opcional, por defecto: true)
# true  = Expulsar bots automáticamente
# false = Aplicar challenge a bots como usuarios normales
BAN_BOTS_DIRECTLY=true

# Tiempo en segundos para eliminar mensajes del desafío (opcional, por defecto: 30)
# Después de completar un desafío, todos los mensajes relacionados se eliminan automáticamente
MESSAGE_CLEANUP_DELAY_SECONDS=30